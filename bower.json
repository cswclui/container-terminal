{
  "name": "kubernetes-container-terminal",
  "version": "1.0.3",
  "description": "Provides a terminal for a kubernetes container in a pod.",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "kubernetes",
    "angularjs",
    "termjs"
  ],
  "authors": [
    "Stef Walter <stefw@redhat.com>"
  ],
  "ignore": [
    "**/.*",
    "Gruntfile.js",
    "*.html",
    "package.json",
    "/container-terminal.*",
    "node_modules",
    "bower_components",
    "scratch"
  ],
  "main": [
    "dist/container-terminal.js",
    "dist/container-terminal.css"
  ],
  "dependencies": {
    "angular": ">=1.3.8 <1.6",
    "term.js": "#0.0.7",
    "font-awesome": "*",
    "kubernetes-container-terminal": "^1.0.3"
  },
  "devDependencies": {
    "patternfly": ">=1.2.1"
  },
  "overrides": {
    "term.js": {
      "main": [
        "src/term.js"
      ]
    }
  }
}
